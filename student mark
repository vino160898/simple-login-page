print("student marks")
i=int(input("enter your subjects : "))
mark=list()
for marks in range(i):
	marks=int(input("enter mark : "))
	mark.append(int(marks))
print(mark)
print("your highestğŸ˜²ï¸ score in :",max(mark))
print("your averageğŸ˜†ï¸ score in :",min(mark))
print("your total score",sum(mark),"Good Luck...ğŸ‘ï¸")
for i in range(len(mark)):
	if mark[i]%2==0:
		print("your even scores:",mark[i])
